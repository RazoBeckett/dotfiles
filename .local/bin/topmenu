#!/bin/env bash

topmenu() {
	options=" Terminal\n󰈹 Firefox\n Files\n󰀴 Android Studio\n󰍜 PowerMenu\n󰜺 Cancel"
	pointer="$(echo -e "$options" | dmenu -fn "JetBrainsMono Nerd Font")"
	case $pointer in
	" Terminal") alacritty ;;
	"󰈹 Firefox") firefox ;;
	" Files") pcmanfm ;;
	"󰀴 Android Studio") android-studio ;;
	"󰍜 PowerMenu") ~/.local/bin/pm ;;
	"󰜺 Cancel") return ;;
	esac
}

topmenu
